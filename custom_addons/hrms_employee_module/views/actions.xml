    <!-- Action: HR Job Position-->
    <record id='action_hr_job_position' model='ir.actions.act_window'>
        <field name='name'>Job Position</field>
        <field name='type'>ir.actions.act_window</field>
        <field name='res_model'>hr.job.positions</field>
        <field name='view_mode'>kanban,list,form</field>
    </record>
</odoo>