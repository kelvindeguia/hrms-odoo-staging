     <!-- Action: HR Contracts-->
    <record id='action_hr_contracts' model='ir.actions.act_window'>
        <field name='name'>Contracts</field>
        <field name='type'>ir.actions.act_window</field>
        <field name='res_model'>hr.contracts</field>
        <field name='view_mode'>list,form,kanban,activity</field>
    </record>

    <!-- Action: HR Departments-->
    <record id='action_hr_departments' model='ir.actions.act_window'>
        <field name='name'>Departments</field>
        <field name='type'>ir.actions.act_window</field>
        <field name='res_model'>hr.departments</field>
        <field name='view_mode'>kanban,list,form</field>
        <!-- hierarchy -->
    </record>

    <!-- Action: HR Dependency-->
    <record id='action_hr_dependency' model='ir.actions.act_window'>
        <field name='name'>Dependency</field>
        <field name='type'>ir.actions.act_window</field>
        <field name='res_model'>hr.dependency</field>
